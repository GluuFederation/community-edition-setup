JAVA_HOME=%(jre_home)s
JAVA=$JAVA_HOME/bin/java
JAVA_OPTIONS="-server -Xms%(scim_min_heap_mem)sm -Xmx%(scim_max_heap_mem)sm -XX:+DisableExplicitGC -Dgluu.base=%(gluuBaseFolder)s -Dserver.base=%(jetty_base)s/scim -Dlog.base=%(jetty_base)s/scim -Dpython.home=%(jython_home)s  -Djavax.net.ssl.trustStore=%(defaultTrustStoreFN)s -Djavax.net.ssl.trustStorePassword=changeit"

JETTY_HOME=%(jetty_home)s
JETTY_BASE=%(jetty_base)s/scim
JETTY_USER=%(jetty_user)s
JETTY_ARGS="jetty.http.host=localhost jetty.http.port=8087"
TMPDIR=%(jetty_dist)s/temp

export PYTHON_HOME=%(jython_home)s

